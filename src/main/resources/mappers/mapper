1. Boardlist_Report
 - Table 명 : REPORT
 - 관련 Query문
 1)
CREATE TABLE REPORT
(
REPORT_IDX NUMBER PRIMARY KEY,
TARGET_U_IDX NUMBER NOT NULL,
REPORT_DATETIME DATE NOT NULL,
ROOM_IDX NUMBER NOT NULL,
U_IDX NUMBER NOT NULL,
REPORT_TYPE NUMBER NOT NULL,
REPORT_DELETE NUMBER DEFAULT 0
);

 2)
CREATE SEQUENCE REPORT_SEQ
INCREMENT BY 1
START WITH 1
MINVALUE 1
MAXVALUE 9999
CYCLE
NOCACHE
NOORDER;

 3)
INSERT INTO REPORT(REPORT_IDX, TARGET_U_IDX, REPORT_DATETIME, ROOM_IDX, U_IDX, REPORT_TYPE)
VALUES (REPORT_SEQ.NEXTVAL
, 11
, SYSDATE
, 12
, 13
, 1000000
);

2. Boardlist_Member
 - Table 명 : user
 - 관련 Query문
 
 1)
CREATE TABLE USERS
(
U_IDX NUMBER PRIMARY KEY,
U_EMAIL VARCHAR2(50) NOT NULL,
U_PWD VARCHAR2(50) NOT NULL,
U_SALT VARCHAR2(50) NOT NULL,
U_NAME VARCHAR2(50) NOT NULL,
U_GENDER VARCHAR2(50) NOT NULL,
U_BDAY VARCHAR2(50) NOT NULL,
U_NICKNAME VARCHAR2(50) NOT NULL,
U_REGION VARCHAR2(50),
U_PTIME VARCHAR2(50),
U_SNS_TYPE VARCHAR2(50),
U_STATUS NUMBER DEFAULT 0,
U_END_DATETIME DATE,
ADMIN_IDX NUMBER NOT NULL,
U_LIKE_CATEGORY NUMBER
U_CERT NUMBER DEFAULT 0,
U_PTIME2 VARCHAR2(50),
U_HASH VARCHAR2(50)
);

 2)
CREATE SEQUENCE USERS_SEQ
INCREMENT BY 1
START WITH 1
MINVALUE 1
MAXVALUE 9999
CYCLE
NOCACHE
NOORDER;

 3)
INSERT INTO USERS(U_IDX, U_EMAIL, U_PWD, U_SALT, U_NAME, U_GENDER, U_BDAY, U_NICKNAME, U_REGION, U_PTIME, U_SNS_TYPE, U_END_DATETIME, ADMIN_IDX)
